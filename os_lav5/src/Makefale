files: pr1 pr2

pr1: libd1.so prog1.cpp
	g++ prog1.cpp -L. -ld1 -o pr1 -Wl,-rpath -Wl,.

pr2: libd1.so libd2.so pr2.cpp
	g++ prog2.cpp -L. -ldl -o pr2 -Wl,-rpath -Wl,.

libd1.so: l1.o 
	g++ -shared l1.o -o libd1.so 

libd2.so: l2.o 
	g++ -shared l2.o -o libd2.so 

l1.o: first.cpp
	g++ -fPIC -c libary1.cpp -o l1.o

l2.o: second.cpp
	g++ -fPIC -c libary2.cpp -o l2.o

clean:
	rm -r *.so *.o pr1 pr2
